    "hrms_base.MailTemplate": [
        {
            "id": 1,
            "name": "Leave Request Submitted",
            "code": "leave_request_submitted",
            "subject": "Leave Request Submitted: {{employee_name}}",
            "body_html": "<p>Dear {{approver_name}},</p><p>A leave request has been submitted by <strong>{{employee_name}}</strong> for your approval.</p><p><strong>Leave Type:</strong> {{leave_type}}<br><strong>From:</strong> {{start_date}}<br><strong>To:</strong> {{end_date}}<br><strong>Days:</strong> {{days_count}}<br><strong>Reason:</strong> {{reason}}</p><p>Please <a href='{{approval_link}}'>click here</a> to review and take action.</p><p>Best regards,<br>HR System</p>",
            "body_text": "Dear {{approver_name}},\n\nA leave request has been submitted by {{employee_name}} for your approval.\n\nLeave Type: {{leave_type}}\nFrom: {{start_date}}\nTo: {{end_date}}\nDays: {{days_count}}\nReason: {{reason}}\n\nPlease login to the HR system to review and take action.\n\nBest regards,\nHR System",
            "model_name": "leave_request",
            "trigger_event": "on_submit",
            "is_active": true,
            "company_id": 1
        },
        {
            "id": 2,
            "name": "Leave Request Approved",
            "code": "leave_request_approved",
            "subject": "Leave Request Approved",
            "body_html": "<p>Dear {{employee_name}},</p><p>Your leave request has been <strong style='color:green'>APPROVED</strong>.</p><p><strong>Leave Type:</strong> {{leave_type}}<br><strong>From:</strong> {{start_date}}<br><strong>To:</strong> {{end_date}}<br><strong>Days:</strong> {{days_count}}</p><p>Approved by: {{approver_name}}</p><p>Best regards,<br>HR System</p>",
            "body_text": "Dear {{employee_name}},\n\nYour leave request has been APPROVED.\n\nLeave Type: {{leave_type}}\nFrom: {{start_date}}\nTo: {{end_date}}\nDays: {{days_count}}\n\nApproved by: {{approver_name}}\n\nBest regards,\nHR System",
            "model_name": "leave_request",
            "trigger_event": "on_approve",
            "is_active": true,
            "company_id": 1
        },
        {
            "id": 3,
            "name": "Leave Request Rejected",
            "code": "leave_request_rejected",
            "subject": "Leave Request Rejected",
            "body_html": "<p>Dear {{employee_name}},</p><p>Your leave request has been <strong style='color:red'>REJECTED</strong>.</p><p><strong>Leave Type:</strong> {{leave_type}}<br><strong>From:</strong> {{start_date}}<br><strong>To:</strong> {{end_date}}<br><strong>Days:</strong> {{days_count}}</p><p>Rejected by: {{approver_name}}<br><strong>Reason:</strong> {{rejection_reason}}</p><p>Best regards,<br>HR System</p>",
            "body_text": "Dear {{employee_name}},\n\nYour leave request has been REJECTED.\n\nLeave Type: {{leave_type}}\nFrom: {{start_date}}\nTo: {{end_date}}\nDays: {{days_count}}\n\nRejected by: {{approver_name}}\nReason: {{rejection_reason}}\n\nBest regards,\nHR System",
            "model_name": "leave_request",
            "trigger_event": "on_reject",
            "is_active": true,
            "company_id": 1
        },
        {
            "id": 4,
            "name": "New Employee Welcome",
            "code": "new_employee_welcome",
            "subject": "Welcome to {{company_name}}!",
            "body_html": "<p>Dear {{employee_name}},</p><p>Welcome to <strong>{{company_name}}</strong>! We are excited to have you join our team.</p><p><strong>Your Details:</strong><br>Employee ID: {{employee_id}}<br>Department: {{department}}<br>Position: {{position}}<br>Start Date: {{start_date}}<br>Reporting Manager: {{manager_name}}</p><p>Your login credentials have been sent in a separate email. Please complete your onboarding tasks in the HR portal.</p><p>If you have any questions, please reach out to HR.</p><p>Best regards,<br>HR Team</p>",
            "body_text": "Dear {{employee_name}},\n\nWelcome to {{company_name}}! We are excited to have you join our team.\n\nYour Details:\nEmployee ID: {{employee_id}}\nDepartment: {{department}}\nPosition: {{position}}\nStart Date: {{start_date}}\nReporting Manager: {{manager_name}}\n\nYour login credentials have been sent in a separate email. Please complete your onboarding tasks in the HR portal.\n\nIf you have any questions, please reach out to HR.\n\nBest regards,\nHR Team",
            "model_name": "employee",
            "trigger_event": "on_create",
            "is_active": true,
            "company_id": 1
        },
        {
            "id": 5,
            "name": "Expense Claim Submitted",
            "code": "expense_claim_submitted",
            "subject": "Expense Claim Submitted: {{employee_name}} - {{amount}} {{currency}}",
            "body_html": "<p>Dear {{approver_name}},</p><p>An expense claim has been submitted by <strong>{{employee_name}}</strong> for your approval.</p><p><strong>Amount:</strong> {{amount}} {{currency}}<br><strong>Category:</strong> {{category}}<br><strong>Description:</strong> {{description}}<br><strong>Date:</strong> {{expense_date}}</p><p>Please <a href='{{approval_link}}'>click here</a> to review and take action.</p><p>Best regards,<br>HR System</p>",
            "body_text": "Dear {{approver_name}},\n\nAn expense claim has been submitted by {{employee_name}} for your approval.\n\nAmount: {{amount}} {{currency}}\nCategory: {{category}}\nDescription: {{description}}\nDate: {{expense_date}}\n\nPlease login to the HR system to review and take action.\n\nBest regards,\nHR System",
            "model_name": "expense_claim",
            "trigger_event": "on_submit",
            "is_active": true,
            "company_id": 1
        }
    ]
