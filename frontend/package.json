{
  "name": "frontend",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "start:dev": "cross-env NODE_ENV=development BYPASS_HTTPS_CHECK=true BYPASS_XSS_DETECTION=true next start",
    "lint": "biome check .",
    "biome": "biome check .",
    "biome:fix": "biome check --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "security:audit": "node scripts/security-audit.js",
    "security:bundle": "node scripts/bundle-analyzer.js",
    "security:full": "npm run security:audit && npm run security:bundle",
    "analyze": "cross-env ANALYZE=true next build",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "graphql:watch": "graphql-codegen --config codegen.yml --watch",
    "prepare": "husky"
  },
  "dependencies": {
