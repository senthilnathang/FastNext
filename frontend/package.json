    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19",
    "@types/xlsx": "^0.0.36",
    "babel-plugin-react-compiler": "^19.0.0-beta.0",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "node scripts/check-debug-code.js",
      "biome check --fix --no-errors-on-unmatched"
    ],
    "*.{json,md}": [
      "biome format --write"
    ]
  },
  "overrides": {
    "prismjs": "^1.30.0"
  }
}
